{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/mediawiki",
		"wikimedia/jquery"
	],
	"globals": {
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"bs": "readonly"
	},
	"rules": {
		"mediawiki/msg-doc": "warn",
		"no-prototype-builtins": "warn",
		"no-shadow-restricted-names": "warn",
		"no-undef": "warn",
		"no-unused-vars": "warn"
	}
}
